{
	"$id": "initialize_fleet_rq.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Initialize fleet",
	"description": "Schema definition to Initialize a fleet",
	"type": "array",
	"examples": [
		[
			{
				  "id": 1,
				  "seats": 4
			  },
			  {
				  "id": 2,
				  "seats": 6
			  }
		  ]
	],
	"items": {
		"$ref": "#/definitions/evs"
	},
	"definitions": {
		"evs": {
			"type": "object",
			"required": [
				"id",
				"seats"
			],
			"properties": {
				"id": {
					"type": "integer",
					"description": "ev id",
					"minimum": 1
				},
				"seats": {
					"type": "integer",
					"description": "ev seats",
					"enum": [
						4,
						5,
						6
					]
				}
			}
		}
	}
}