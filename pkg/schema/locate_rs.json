{
	"$id": "initialize_fleet_rq.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Initialize fleet",
	"description": "Schema definition to Initialize a fleet",
	"type": "object",
	"examples": [
		{
			"id": 1,
			"seats": 4
		}
	],
	"properties": {
		"id": {
			"type": "string",
			"description": "car uuid",
			"minimum": 1
		},
		"seats": {
			"type": "integer",
			"description": "ev seats",
			"enum": [
				4,
				5,
				6
			]
		}
	},
	"required": [
		"id",
		"seats"
	]
}